
#include <iostream>
#include <map>
using namespace std;

int main() {
	map<char, int> first;
	map<char, int> second;
	int nums;
	string first_word, second_word;
	cin >> nums;
	while(nums > 0)
	{
		cin >> first_word >> second_word;
		for(const auto& s: first_word)
		{
			first[s]+= 1;
		}
	}
	return 0;
}
