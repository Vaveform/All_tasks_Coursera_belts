void dump(const map<string,string>& countries)
{
	if(countries.size() == 0)
	{
		cout << "There are no countries in the world" << endl;
	}
	else
	{
		for(const auto& item: countries)
		{
			cout << item.first << "/" << item.second << " ";
		}
	}
	cout << endl;
}
int main()
{
	map<string, string> countries;
	dump
	return 0;
}
