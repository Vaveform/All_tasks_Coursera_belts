#include <iostream>
#include <string>
#include <cmath>

using namespace std;

int main()
{
	float N;
	float A, B, X, Y;
	cin >> N >> A >> B >> X >> Y;
	if(N >= A && N < B)
	{
		cout << N - N * (X/100) << endl;
	}
	else if(N >= B)
	{
		cout << N - N * (Y/100) << endl;
	}
	else
	{
		cout << N << endl;
	}
}
