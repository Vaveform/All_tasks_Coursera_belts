

#include <vector>
#include <iostream>

using namespace std;

template<typename T> class Table{
public:
	vector<vector<T>> table;
public:
	Table(size_t nums_of_rows, size_t nums_of_columns){
		table(nums_of_rows);
		for(auto& elem: table){
			elem(nums_of_columns);
		}
	}
};



int main() {
	Table<int> my_table(4,4);
	cout << my_table
  return 0;
}
